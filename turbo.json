{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "blog": {
      "cache": false
    },
    "blog:build": {
      "dependsOn": [
        "vue-comp#build",
        "jsonl-core#build",
        "@jsonl/rspack#build",
        "@jsonl/rsbuild#build",
     
        "rspress-md-tojsonl#build",
        "^build"
      ],
      "cache": false
    },
    "stub": {
      "cache": false
    },
    "ant:t": {
      "cache": false
    },
    "dev:plugin": {
      "cache": false
    }
  }
}